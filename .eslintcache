\n            <div id=\"inventory_page_container\">\r\n                    <div className=\"page_title\">{Dictionary[\"inventory\"] + \" | \"+str}</div>\r\n                    <div id=\"first_notification\"></div>\r\n                    \r\n            </div>\r\n            \r\n        );\r\n\r\n    }\r\n}\r\nexport default InventoryPage\r\n\r\nexport class AddToOrder extends Component {\r\n    constructor(props) {\r\n      super(props);\r\n      this.state = {\r\n        show: false,\r\n        kind:props.kind\r\n      };\r\n      this.close = this.close.bind(this);\r\n      this.open = this.open.bind(this);\r\n    }\r\n    close() {\r\n      this.setState({\r\n        show: false\r\n      });\r\n    }\r\n    open(size) {\r\n      this.setState({\r\n        size,\r\n        show: true\r\n      });\r\n    }\r\n    render() {\r\n        var type =\"\"\r\n        if(this.state.kind == 0)\r\n            type = <img src={cart_plus} onClick={() => this.open('xs')} style={{\"cursor\":\"pointer\"}} />\r\n        else if(this.state.kind == 1)\r\n            type = <div className=\"add_to_order\" onClick={() => this.open('xs')} >{Dictionary.add_to_order}</div>\r\n\r\n      return (\r\n        <div className=\"add_to_cart_modal_container\">\r\n          <ButtonToolbar>\r\n            {/* <Button size=\"xs\" onClick={() => this.open('xs')}>\r\n              Xsmall\r\n            </Button> */}\r\n            {/* <img src={cart_plus} onClick={() => this.open('xs')} style={{\"cursor\":\"pointer\"}} /> */}\r\n            {type}\r\n          </ButtonToolbar>\r\n          <Modal  size={this.state.size} show={this.state.show} onHide={this.close}>\r\n            <Modal.Header>\r\n              <Modal.Title>Modal Title</Modal.Title>\r\n            </Modal.Header>\r\n            <Modal.Body>\r\n              <Quantity />\r\n            </Modal.Body>\r\n            <Modal.Footer>\r\n              <Button onClick={this.close} appearance=\"primary\">\r\n                {Dictionary[\"add_to